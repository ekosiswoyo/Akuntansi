	<queryString>
		<![CDATA[SELECT
     SUM(kas_masuk.NOMINAL) AS JUMLAHMASUK,
     SUM(kas_keluar.NOMINAL) AS JUMLAHKELUAR,
     kas_masuk.`keterangan` AS kas_masuk_keterangan,
     kas_masuk.`nominal` AS kas_masuk_nominal,
     kas_keluar.`keterangan` AS kas_keluar_keterangan,
     kas_keluar.`nominal` AS kas_keluar_nominal
FROM
     `kas_masuk` kas_masuk,
     `kas_keluar` kas_keluar
WHERE
     kas_masuk.TGL_NOTA BETWEEN $P{parTglAwal} AND $P{parTglAkhir}
 AND kas_keluar.TGL_NOTA BETWEEN $P{parTglAwal} AND $P{parTglAkhir}]]>
	</queryString>
